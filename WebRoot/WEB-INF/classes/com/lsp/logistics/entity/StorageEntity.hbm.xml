<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lsp.logistics.entity.StorageEntity" table="t_storage">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="storageId" type="java.lang.String">
			<column name="storage_id" />
		</property>
		<property name="storageName" type="java.lang.String">
			<column name="storage_name" />
		</property>
		<property name="storageAddress" type="java.lang.String">
			<column name="storage_address" />
		</property>
		<property name="storageManagement" type="java.lang.String">
			<column name="storage_management" />
		</property>
		<property name="storageLevel" type="java.lang.String">
			<column name="storage_level" />
		</property>
	</class>
</hibernate-mapping>
