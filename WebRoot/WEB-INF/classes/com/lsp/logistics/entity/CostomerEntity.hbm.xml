<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.lsp.logistics.entity.CostomerEntity" table="t_costomer">
		<id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="costomerName" type="java.lang.String">
        	 <column name="costomerName"/>
        </property>
        <property name="costomerId" type="java.lang.String">
        	 <column name="costomerId"/>
        </property>
        <property name="company" type="java.lang.String">
        	 <column name="company"/>
        </property>
        <property name="mobailPhone" type="java.lang.String">
        	 <column name="mobailPhone"/>
        </property>
        <property name="phone" type="java.lang.String">
        	 <column name="phone"/>
        </property>
        <property name="address" type="java.lang.String">
        	 <column name="address"/>
        </property>
        <property name="postcode" type="java.lang.String">
        	 <column name="postcode"/>
        </property>
        <property name="email" type="java.lang.String">
        	 <column name="email"/>
        </property>
        
	</class>	
</hibernate-mapping>
