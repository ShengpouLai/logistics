<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.lsp.logistics.entity.FirstGoodsEntity" table="t_first_goods">
		<id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="firstGoodsName" type="java.lang.String">
        	 <column name="firstGoodsName"/>
        </property>
        <property name="firstGoodsInfo" type="java.lang.String">
        	 <column name="firstGoodsInfo"/>
        </property>
	</class>
</hibernate-mapping>
